1. #include <stdlib.h> 
 2. #include <stdio.h> 
 3. #include “fibo.h” 
 4.   
 5. Int main (int argc, char * argv []) 
 6. { 
 7. Long ret; 
 8. Int n; 
 9.  If (argc<2){ 
10. Fprintf (stderr, “missing index.\n”); 
11. Return -1; 
12. } 
13. N=atoi (argv [1]); 
14. Ret = fibo (n); 
15. Printf (“Fibo (%d) is %ld\n”,n,ret); 
16. Return EXIT_SUCCESS; 
17. }
